{
	"name": "Block Bad Agency",
	"description": "Отображает информацию о недобросовестных риелторах, информ-агенствах и лохотронах. Основано на базе телефонов собранных порталом lun.ua ", 
	"version": "0.1",
	"permissions": [
		"http://www.lun.ua/*"
	],
	"background": {
		"scripts": ["background.js"]
	},
	"icons": {
		"128": "icon.png",
		"38": "icon.png"
		},
	"browser_action": {
		"default_title": "Block Bad Agency", 
		"default_popup": "main.html"
	}, 

	"manifest_version": 2,
	"update_url": "http://clients2.google.com/service/update2/crx",
	"content_security_policy": "script-src 'self' https://www.google.com; object-src 'self'"
	


}